{
  "name": "@zaifer/peachui",
  "version": "0.2.35",
  "author": "zaifer",
  "description": "Vue component library for easy web development.",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zaif3r/peachui.git"
  },
  "main": "./dist/peachui.umd.js",
  "module": "./dist/peachui.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src/runtime"
  ],
  "exports": {
    ".": {
      "import": "./dist/peachui.es.js",
      "require": "./dist/peachui.cjs.js"
    },
    "./nuxt": {
      "import": "./dist/peachui-nuxt.es.js",
      "require": "./dist/peachui-nuxt.cjs.js"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.13",
    "@vueuse/core": "^9.6.0",
    "daisyui": "^2.41.0",
    "swiper": "^8.4.5",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@nuxt/kit": "^3.0.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.13",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "postcss": "^8.4.19",
    "rollup-plugin-typescript2": "^0.34.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "vue",
    "tailwind",
    "daisyui",
    "components",
    "peachui"
  ]
}
